{
    "name": "archives",
    "version": "1.0.0",
    "description": "Fast to deploy, easy to use, and completely free archives for whatever you want to store.",
    "main": "index.js",
    "private": true,
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "format": "prettier --write . --ignore-path .gitignore",
        "eslint": "eslint . --cache --cache-location .cache/.eslintcache --report-unused-disable-directives --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
        "eslint-fix": "eslint --fix . --cache --cache-location .cache/.eslintcache --report-unused-disable-directives --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
        "stylelint": "stylelint src/assets/scss/. --cache --cache-location .cache/.stylelintcache --ignore-path .gitignore",
        "stylelint-fix": "stylelint --fix src/assets/scss/. --cache --cache-location .cache/.stylelintcache --ignore-path .gitignore",
        "lint": "npm-run-all --parallel eslint stylelint",
        "lint-fix": "npm-run-all --parallel eslint-fix stylelint-fix",
        "format-and-lint": "npm-run-all --parallel format lint",
        "format-and-lint-fix": "npm-run-all --parallel format lint-fix",
        "web-build": "webpack --config webpack.prod.js",
        "web-dev": "webpack --config webpack.dev.js",
        "web-server": "webpack-dev-server --open --config webpack.dev.js",
        "web-test": "npm-run-all --parallel web-test-*",
        "web-test-unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js",
        "server-test": "exit 0",
        "build": "npm-run-all --parallel web-build",
        "test": "npm-run-all --parallel web-test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Yuba-Technology/Archives.git"
    },
    "keywords": [
        "Archives",
        "Archive",
        "web",
        "frontend"
    ],
    "author": "Yuba Technology, Archives contributors and You",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Yuba-Technology/Archives/issues"
    },
    "homepage": "https://github.com/Yuba-Technology/Archives#readme",
    "dependencies": {
        "@types/d3-scale-chromatic": "^3.0.3",
        "bootstrap": "^5.3.3",
        "element-plus": "^2.5.5",
        "linkify-it": "^5.0.0",
        "markdown-it": "^14.0.0",
        "markdown-it-abbr": "^2.0.0",
        "markdown-it-cjk-breaks": "^2.0.0",
        "markdown-it-container": "^4.0.0",
        "markdown-it-deflist": "^3.0.0",
        "markdown-it-emoji": "^3.0.0",
        "markdown-it-footnote": "^4.0.0",
        "markdown-it-for-inline": "^2.0.1",
        "markdown-it-ins": "^4.0.0",
        "markdown-it-mark": "^4.0.0",
        "markdown-it-sub": "^2.0.0",
        "markdown-it-sup": "^2.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.23.9",
        "@babel/plugin-transform-modules-commonjs": "^7.23.3",
        "@babel/preset-env": "^7.24.0",
        "@rushstack/eslint-patch": "^1.7.2",
        "@testing-library/dom": "^9.3.4",
        "@testing-library/react": "^14.2.1",
        "@types/jest": "^29.5.12",
        "@vue/compiler-sfc": "^3.4.25",
        "@vue/eslint-config-prettier": "^9.0.0",
        "assert": "^2.1.0",
        "babel-loader": "^9.1.3",
        "babel-preset-env": "^0.0.0",
        "buffer": "^6.0.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^7.1.1",
        "cypress": "^13.8.1",
        "eslint": "^8.57.0",
        "eslint-cli": "^1.1.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-xo": "^0.44.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^28.2.0",
        "eslint-plugin-markdown": "^4.0.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-unicorn": "^51.0.1",
        "eslint-plugin-vue": "^9.23.0",
        "file-loader": "^6.2.0",
        "html-loader": "^5.0.0",
        "html-webpack-plugin": "^5.6.0",
        "https-browserify": "^1.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-transform-yaml": "^1.1.2",
        "js-yaml": "^4.1.0",
        "mini-css-extract-plugin": "^2.9.0",
        "naive-ui": "^2.37.3",
        "node-polyfill-webpack-plugin": "^3.0.0",
        "node-sass": "^9.0.0",
        "nodemon": "^3.0.3",
        "npm-run-all": "^4.1.5",
        "os-browserify": "^0.3.0",
        "prettier": "^3.2.5",
        "process": "^0.11.10",
        "sass-loader": "^14.2.1",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "style-loader": "^4.0.0",
        "stylelint": "^16.3.1",
        "stylelint-cli": "^1.3.0",
        "stylelint-config-standard": "^36.0.0",
        "stylelint-config-standard-scss": "^13.1.0",
        "stylelint-scss": "^6.2.1",
        "terser-webpack-plugin": "^5.3.10",
        "unplugin-auto-import": "^0.17.5",
        "unplugin-vue-components": "^0.26.0",
        "url": "^0.11.3",
        "vfonts": "^0.0.3",
        "vue": "^3.4.21",
        "vue-loader": "^17.4.2",
        "vue-template-compiler": "^2.7.16",
        "webpack": "^5.90.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "^5.10.0",
        "yaml-loader": "^0.8.1"
    }
}
